{
  "name": "create-crispygoat-app",
  "version": "1.0.0",
  "description": "CrispyGoat development template for building modern websites and apps",
  "type": "module",
  "bin": {
    "create-crispygoat-app": "./scripts/create-app.js"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit",
    "docs:build": "typedoc src/index.ts",
    "docs:serve": "typedoc src/index.ts --watch",
    "cms:dev": "cd cms && npm run develop",
    "cms:build": "cd cms && npm run build",
    "setup": "node scripts/setup.js"
  },
  "dependencies": {
    "astro": "4.16.12",
    "@astrojs/react": "3.6.2",
    "@astrojs/tailwind": "5.1.2",
    "@astrojs/vercel": "7.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwindcss": "3.4.16",
    "@tailwindcss/typography": "0.5.15",
    "drizzle-orm": "0.29.5",
    "postgres": "3.4.4",
    "@auth/core": "0.18.6",
    "@auth/astro": "0.3.5",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "8.57.1",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-jsdoc": "46.10.1",
    "typescript": "5.6.3",
    "typedoc": "0.25.16",
    "@inquirer/prompts": "3.3.2",
    "chalk": "5.3.0",
    "ora": "7.0.1",
    "validate-npm-package-name": "5.0.1"
  },
  "keywords": [
    "astro",
    "template",
    "typescript",
    "tailwind",
    "cms",
    "crispygoat"
  ],
  "author": "CrispyGoat",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}